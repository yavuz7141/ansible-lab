---
 - name: Launching aws ec2 instance
   hosts: localhost
   gather_facts: false
   tasks:
   - name: Launching ec2 instance with ec2 module 
     ec2:
      instance_type: t2.micro
      image: ami-013570d303005cc7e
      key_name: newdevops
      region: us-east-1
      wait: yes
       group_id:
        - devops
      instance_tags:
        - Environment: web
        - Name: clientnode5